<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <diskStore path="java.io.tmpdir" />
    <defaultCache maxElementsInMemory="10000"
                  eternal="false"
                  timeToIdleSeconds="1000"
                  timeToLiveSeconds="1000"
                  overflowToDisk="false"
                  memoryStoreEvictionPolicy="LRU"/>
</ehcache>